<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
	<diskStore path="java.io.tmpdir" />
	<defaultCache maxElementsInMemory="1000" eternal="true"
		timeToIdleSeconds="120" timeToLiveSeconds="120" overflowToDisk="false"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="120"
		memoryStoreEvictionPolicy="LRU" />
		
			<!--TM_SYSTEM_DIC -->
	<cache name="SystemDicCache" maxElementsInMemory="3000" eternal="true"
		overflowToDisk="false" timeToIdleSeconds="1800" timeToLiveSeconds="7200"
		memoryStoreEvictionPolicy="LRU">
	</cache>
	<cache name="DATA_DIC_MAP_CACHE" maxElementsInMemory="2000" eternal="true"
		overflowToDisk="false" timeToIdleSeconds="1800" timeToLiveSeconds="7200"
		memoryStoreEvictionPolicy="LRU">
	</cache>
	<!-- fun act opt list cache -->
	<cache name="FUN_ACT_OPT_CACHE" maxElementsInMemory="2000" eternal="true"
		overflowToDisk="false" timeToIdleSeconds="1800" timeToLiveSeconds="7200"
		memoryStoreEvictionPolicy="LRU">
	</cache>
	
	<!-- 一二级菜单缓存 -->
	<cache name="FUNCTION_ACTION_CACHE" maxElementsInMemory="2000" eternal="true"
		overflowToDisk="false" timeToIdleSeconds="1800" timeToLiveSeconds="7200"
		memoryStoreEvictionPolicy="LRU">
	</cache>
	<!-- 二级三级缓存 -->
	<cache name="ACTION_OPTION_CACHE" maxElementsInMemory="2000" eternal="true"
		overflowToDisk="false" timeToIdleSeconds="1800" timeToLiveSeconds="7200"
		memoryStoreEvictionPolicy="LRU">
	</cache>
	
</ehcache> 



